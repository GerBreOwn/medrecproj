SELECT
    `patient_patient`.`last_name`
    , `patient_patient`.`first_name`
    , `patient_patient`.`middle_initial`
    , `patient_patient`.`date_of_birth`
    , `patient_patient`.`gender`
    , `patient_town`.`name`
FROM
    `medrec`.`patient_patient`
    INNER JOIN `medrec`.`patient_town` 
        ON (`patient_patient`.`town_id` = `patient_town`.`id`);
